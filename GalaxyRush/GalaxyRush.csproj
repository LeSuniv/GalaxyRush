  </ItemGroup>

  <ItemGroup>
    <Resource Include="Stargaze Stencil.otf">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="fusee.png" />
